// array
const arr = [1,2,3,4,5];


// function to map
function multiplyBy2 (element){
    return element*2;
}

// getting new array mapped to the function
const newArr = arr.map(multiplyBy2);

// print both arrays
console.log(arr, newArr);

// in short
// console.log(arr.map(element=>element*2));

